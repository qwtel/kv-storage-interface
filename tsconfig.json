{
  "compilerOptions": {
    "outDir": ".",
    "lib": [
      "ES2020",
      "WebWorker",
      "ESNext.AsyncIterable"
    ],
    "target": "ES2019",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "allowJs": true,
